		{"id":"4ae90856d1a5fbb9","type":"text","text":"3 НФ","x":-2851,"y":-1713,"width":250,"height":60,"color":"#000000"},
		{"id":"ff21820023bea62a","type":"file","file":"Cache/Pasted image 20240825164234.png","x":-3780,"y":-1558,"width":400,"height":249,"color":"6"},
