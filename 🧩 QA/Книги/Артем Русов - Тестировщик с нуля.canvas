      "y": 2240
    },
    {
      "height": 60,
      "id": "a92d42cd89435786",
      "styleAttributes": {
      },
      "text": "Название самих проверок",
      "type": "text",
      "width": 250,
      "x": 2977,
      "y": 1920
    },
    {
      "height": 60,
      "id": "e2d9fc3ab27b1124",
